{
	"name": "combineHsl",
	"version": "0.0.1",

	"devDependencies": {
		"browserify": "latest",
		"watchify": "latest"
	},

	"scripts": {
		"compile": "browserify src/combineHsl.js -o combineHsl.js",
		"watchMain": "watchify src/combineHsl.js -o combineHsl.js -v",
		"watchWorker": "watchify src/worker.js -o worker.js -v",
		"watch": "npm run watchMain | npm run watchWorker"
	}
}
